.newheader
  .content
    .logo
      a(href=header.logo.url, title=base.title)
        img(src="assets/img/logo-starsconf-transparent-2017.svg")
    .nav
      a.button-collapse.visible-xs.visible-sm(href='#', data-activates='slide-out')
        i.material-icons menu
      ul.hidden-xs.hidden-sm
        each option, index in header.menu_options
          li
            a(href= option.url)= option.texto

      ul#slide-out.side-nav
        each option, index in header.menu_options
          li
            a(href= option.url)= option.texto
    
