.container-fluid.main-content.keynoters
  .container
    .row.title
      .col-md-12.section
        h2.scrollspy#keynoters= homepage.section_three.title
        p= homepage.section_three.bajada


    .row.people
      each keynoter in homepage.section_three.keynoters
        .col-md-4.col-xs-12.keynoter(class= keynoter.switch)
          .image
            a(href='#' title='Ver detalle de la presentación')
              img(src= keynoter.foto)
          .data
            p= keynoter.nombre
            span= keynoter.bio
            span!= keynoter.contacto

      .col-md-12.section
        p= homepage.section_three.bajada_dos

    .row.people
      each keynoter in homepage.section_three.keynoters2
        .col-md-3.col-xs-12.keynoter2(class= keynoter.switch)
          .image
            a(href='#' title='Ver detalle de la presentación')
              img(src= keynoter.foto)
          .data
            p= keynoter.nombre
            span= keynoter.bio
            span!= keynoter.contacto